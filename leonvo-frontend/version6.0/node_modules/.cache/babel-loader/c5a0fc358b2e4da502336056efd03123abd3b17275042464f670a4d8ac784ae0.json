{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.every.js\";\nimport axios from 'axios';\nimport apiConfig from '@/config/api.config';\nexport default {\n  name: 'UserRegister',\n  data() {\n    return {\n      form: {\n        username: '',\n        email: '',\n        password: '',\n        confirmPassword: '',\n        emailCode: '',\n        agree: false\n      },\n      errors: {\n        username: '',\n        email: '',\n        password: '',\n        confirmPassword: '',\n        emailCode: '',\n        agree: ''\n      },\n      emailStatus: null,\n      countdown: 0,\n      canSend: true,\n      showSuccess: false,\n      countdownTimer: null\n    };\n  },\n  computed: {\n    isFormValid() {\n      return Object.values(this.errors).every(error => !error) && this.form.username && this.form.email && this.form.password && this.form.confirmPassword && this.form.emailCode && this.form.agree;\n    }\n  },\n  methods: {\n    validateUsername() {\n      if (!this.form.username) {\n        this.errors.username = 'Áî®Êà∑Âêç‰∏çËÉΩ‰∏∫Á©∫';\n      } else if (this.form.username.length < 2 || this.form.username.length > 10) {\n        this.errors.username = 'Áî®Êà∑ÂêçÈïøÂ∫¶‰∏∫2-10‰Ωç';\n      } else {\n        this.errors.username = '';\n      }\n    },\n    validateEmail() {\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      if (!this.form.email) {\n        this.errors.email = 'ÈÇÆÁÆ±‰∏çËÉΩ‰∏∫Á©∫';\n      } else if (!emailRegex.test(this.form.email)) {\n        this.errors.email = 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÈÇÆÁÆ±Âú∞ÂùÄ';\n      } else {\n        this.errors.email = '';\n      }\n    },\n    validatePassword() {\n      if (!this.form.password) {\n        this.errors.password = 'ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫';\n      } else if (this.form.password.length < 6 || this.form.password.length > 16) {\n        this.errors.password = 'ÂØÜÁ†ÅÈïøÂ∫¶‰∏∫6-16‰Ωç';\n      } else {\n        this.errors.password = '';\n      }\n    },\n    validateConfirmPassword() {\n      if (!this.form.confirmPassword) {\n        this.errors.confirmPassword = 'ËØ∑Á°ÆËÆ§ÂØÜÁ†Å';\n      } else if (this.form.password !== this.form.confirmPassword) {\n        this.errors.confirmPassword = '‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥';\n      } else {\n        this.errors.confirmPassword = '';\n      }\n    },\n    async sendEmailCode() {\n      this.validateEmail();\n      if (this.errors.email) {\n        return;\n      }\n      this.canSend = false;\n      this.emailStatus = null;\n      try {\n        const response = await axios.post(apiConfig.email.sendCode, {\n          email: this.form.email,\n          purpose: 'register'\n        }, {\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        });\n        const data = response.data || {};\n        if (data.code === 1) {\n          this.emailStatus = {\n            success: true,\n            message: data.msg || 'È™åËØÅÁ†ÅÂ∑≤ÂèëÈÄÅÂà∞ÊÇ®ÁöÑÈÇÆÁÆ±'\n          };\n          this.startCountdown(60);\n        } else {\n          this.emailStatus = {\n            success: false,\n            message: data.msg || 'ÂèëÈÄÅÈ™åËØÅÁ†ÅÂ§±Ë¥•'\n          };\n          this.canSend = true;\n          if (data.data?.remaining) {\n            this.startCountdown(data.data.remaining);\n          }\n        }\n      } catch (error) {\n        console.error('ÂèëÈÄÅÈ™åËØÅÁ†ÅÂ§±Ë¥•:', error);\n        this.emailStatus = {\n          success: false,\n          message: 'ÂèëÈÄÅÈ™åËØÅÁ†ÅÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï'\n        };\n        this.canSend = true;\n      }\n    },\n    startCountdown(seconds) {\n      this.countdown = seconds;\n      this.canSend = false;\n      this.countdownTimer = setInterval(() => {\n        this.countdown--;\n        if (this.countdown <= 0) {\n          clearInterval(this.countdownTimer);\n          this.canSend = true;\n        }\n      }, 1000);\n    },\n    async handleRegister() {\n      this.validateUsername();\n      this.validateEmail();\n      this.validatePassword();\n      this.validateConfirmPassword();\n      if (!this.form.agree) {\n        this.errors.agree = 'ËØ∑ÂêåÊÑèÁî®Êà∑ÂçèËÆÆÂíåÈöêÁßÅÊîøÁ≠ñ';\n        return;\n      } else {\n        this.errors.agree = '';\n      }\n      if (!this.isFormValid) {\n        return;\n      }\n      if (!this.form.emailCode || this.form.emailCode.length !== 6) {\n        this.errors.emailCode = 'ËØ∑ËæìÂÖ•6‰ΩçÈ™åËØÅÁ†Å';\n        return;\n      }\n      try {\n        const response = await axios.post(apiConfig.email.register, {\n          email: this.form.email,\n          password: this.form.password,\n          confirmPassword: this.form.confirmPassword,\n          code: this.form.emailCode,\n          username: this.form.username || undefined,\n          phone: ''\n        }, {\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        });\n        const data = response.data || {};\n        if (data.code === 1) {\n          this.showSuccess = true;\n          setTimeout(() => {\n            this.$router.push('/login');\n          }, 3000);\n        } else {\n          const msg = data.msg || 'Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï';\n          alert('Ê≥®ÂÜåÂ§±Ë¥•: ' + msg);\n          if (msg.includes('È™åËØÅÁ†Å')) {\n            this.errors.emailCode = msg;\n          } else if (msg.includes('Áî®Êà∑Âêç')) {\n            this.errors.username = msg;\n          } else if (msg.includes('ÈÇÆÁÆ±')) {\n            this.errors.email = msg;\n          }\n        }\n      } catch (error) {\n        console.error('Ê≥®ÂÜåÂ§±Ë¥•:', error);\n        alert('Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï');\n      }\n    }\n  },\n  mounted() {\n    console.log('EmailRegister component mounted');\n  },\n  beforeUnmount() {\n    if (this.countdownTimer) {\n      clearInterval(this.countdownTimer);\n    }\n  }\n};","map":{"version":3,"names":["axios","apiConfig","name","data","form","username",