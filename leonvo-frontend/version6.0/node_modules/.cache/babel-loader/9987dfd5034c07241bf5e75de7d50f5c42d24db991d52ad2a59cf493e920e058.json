iv class=\"search-box\">\r\n          <input\r\n              type=\"text\"\r\n              v-model=\"productSearch\"\r\n              placeholder=\"ÊêúÁ¥¢ÂïÜÂìÅÂêçÁß∞...\"\r\n              class=\"search-input\"\r\n          />\r\n          <span class=\"search-icon\">Êêú</span>\r\n        </div>\r\n\r\n        <!-- ÁÆÄÂçïÁöÑÁ≠õÈÄâÊåâÈíÆ -->\r\n        <button\r\n            class=\"btn-filter\"\r\n            :class=\"{ active: showLowStock }\"\r\n            @click=\"showLowStock = !showLowStock\"\r\n        >\r\n          <span class=\"filter-icon\">‚ö†Ô∏è</span>\r\n          ‰ΩéÂ∫ìÂ≠ò\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Âä†ËΩΩÁä∂ÊÄÅ -->\r\n    <div v-if=\"loading\" class=\"loading-state\">\r\n      <div class=\"spinner\"></div>\r\n      Ê≠£Âú®Âä†ËΩΩÂïÜÂìÅÊï∞ÊçÆ...\r\n    </div>\r\n\r\n    <!-- ÈîôËØØÁä∂ÊÄÅ -->\r\n    <div v-else-if=\"error\" class=\"error-state\">\r\n      Âä†ËΩΩÂ§±Ë¥•Ôºö{{ error }}\r\n      <button @click=\"loadProducts\" class=\"btn-retry\">ÈáçËØï</button>\r\n    </div>\r\n\r\n    <div v-else class=\"table-container\">\r\n      <table class=\"data-table\">\r\n        <thead>\r\n        <tr>\r\n          <th>ID</th>\r\n          <th class=\"text-left\">ÂïÜÂìÅÂêçÁß∞</th>\r\n          <th class=\"text-left\">‰ª∑Ê†º</th>\r\n          <th class=\"text-left\">Â∫ìÂ≠ò</th>\r\n          <th class=\"text-left\">CPU</th>\r\n          <th class=\"text-left\">ÊòæÂç°</th>\r\n          <th class=\"text-left\">ËØÑÂàÜ</th>\r\n          <th>Êìç‰Ωú</th>\r\n        </tr>\r\n        </thead>\r\n        <tbody>\r\n        <tr v-for=\"product in paginatedProducts\" :key=\"product.pid\">\r\n          <td><span class=\"id-text\">#{{ product.pid }}</span></td>\r\n          <td class=\"text-left\">\r\n            <div class=\"product-cell\">\r\n              <!-- ÁÆÄÂçïÁöÑÊñáÂ≠óÂõæÊ†á‰ª£ÊõøÂõæÁâá -->\r\n              <div class=\"product-icon\">üíª</div>\r\n              <div class=\"product-info\">\r\n                <div class=\"product-name\">{{ product.name }}</div>\r\n               \r\n              </div>\r\n            </div>\r\n          </td>\r\n          <td class=\"text-left\"><strong class=\"price-text\">¬•{{ product.price }}</strong></td>\r\n          <td class=\"text-left\">\r\n              <span :class=\"['stock-badge', getStockLevel(product.stock)]\">\r\n                {{ product.stock }}‰ª∂\r\n              </span>\r\n          </td>\r\n          <td class=\"text-left\">{{ product.CPU || '-' }}</td>\r\n          <td class=\"text-left\">{{ product.GPU || '-' }}</td>\r\n          <td class=\"text-left\">\r\n            <div class=\"rating\">\r\n              <span class=\"rating-value\">{{ product.rating }}</span>\r\n              <span class=\"rating-star\">‚òÖ</span>\r\n            </div>\r\n          </td>\r\n          <td>\r\n            <div class=\"action-buttons center-content\">\r\n              <button class=\"btn-view\" @click=\"viewProduct(product)\">Êü•Áúã</button>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n    <!-- Â∫ïÈÉ®‰ø°ÊÅØ -->\r\n    <div class=\"table-footer\">\r\n      <div class=\"pagination-info\">\r\n        ÊòæÁ§∫ {{ filteredProducts.length }} Êù°ËÆ∞ÂΩï‰∏≠ÁöÑ {{ filteredProducts.length > 0 ? (currentPage - 1) * pageSize + 1 : 0 }}-{{ Math.min(currentPage * pageSize, filteredProducts.length) }} Êù°\r\n        <span v-if=\"showLowStock\" class=\"filter-hint\">Ôºà‰ΩéÂ∫ìÂ≠òÁ≠õÈÄâÔºâ</span>\r\n        <span v-if=\"isMockData\" class=\"mock-hint\">„ÄêÂΩìÂâç‰∏∫Ê®°ÊãüÊï∞ÊçÆ„Äë</span>\r\n      </div>\r\n      <div class=\"pagination\">\r\n        <button class=\"pagination-btn\" :disabled=\"currentPage === 1\" @click=\"prevPage\">‰∏ä‰∏ÄÈ°µ</button>\r\n        <span class=\"pagination-current\">{{ currentPage }} / {{ totalPages || 1 }}</span>\r\n        <button class=\"pagination-btn\" :disabled=\"currentPage === totalPages || totalPages === 0\" @click=\"nextPage\">‰∏ã‰∏ÄÈ°µ</button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Êü•ÁúãÂïÜÂìÅÊ®°ÊÄÅÊ°ÜÔºàÁÆÄÂåñÁâàÔºâ -->\r\n    <div v-if=\"selectedProduct\" class=\"modal-overlay\" @click.self=\"selectedProduct = null\">\r\n      <div class=\"modal\">\r\n        <div class=\"modal-header\">\r\n          <h3>ÂïÜÂìÅËØ¶ÊÉÖ</h3>\r\n          <button class=\"modal-close\" @click=\"selectedProduct = null\">√ó</button>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <div class=\"product-detail\">\r\n            <div class=\"detail-section\">\r\n              <h4>{{ selectedProduct.name }}</h4>\r\n              <div class=\"detail-grid\">\r\n                <div class=\"detail-item\">\r\n                  <label>ÂïÜÂìÅIDÔºö</label>\r\n                  <span>{{ selectedProduct.pid }}</span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <label>‰ª∑Ê†ºÔºö</label>\r\n                  <span class=\"price\">¬•{{ selectedProduct.price }}</span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <label>Â∫ìÂ≠òÔºö</label>\r\n                  <span :class=\"['stock-badge', getStockLevel(selectedProduct.stock)]\">\r\n                    {{ selectedProduct.stock }}‰ª∂\r\n                  </span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <label>CPUÔºö</label>\r\n                  <span>{{ selectedProduct.CPU || '-' }}</span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <label>ÊòæÂç°Ôºö</label>\r\n                  <span>{{ selectedProduct.GPU || '-' }}</span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <label>Á±ªÂûãÔºö</label>\r\n                  <span>{{ selectedProduct.type }}</span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <label>ËØÑÂàÜÔºö</label>\r\n                  <span class=\"rating\">{{ selectedProduct.rating }} ‚òÖ</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport apiConfig from '@/config/api.config'\r\nimport axios from 'axios';\r\n\r\n// Ê®°ÊãüÊï∞ÊçÆ - Â§áÁî®\r\nconst MOCK_PRODUCTS = [\r\n  { pid: 1, name: 'ËÅîÊÉ≥ÊãØÊïëËÄÖY9000P', price: 8999, stock: 15, CPU: 'i7-13700H', GPU: 'RTX 4060', type: 'Ê∏∏ÊàèÊú¨', rating: 4.8 },\r\n  { pid: 2, name: 'ËÅîÊÉ≥Â∞èÊñ∞Pro 14', price: 5499, stock: 45, CPU: 'i5-13500H', GPU: 'Ê†∏Êòæ', type: 'ËΩªËñÑÊú¨', rating: 4.6 },\r\n  { pid: 3, name: 'ËÅîÊÉ≥ThinkPad X1', price: 12999, stock: 8, CPU: 'i7-1360P', GPU: 'Ê†∏Êòæ', type: 'ÂïÜÂä°Êú¨', rating: 4.9 },\r\n  { pid: 4, name: 'ËÅîÊÉ≥ÊãØÊïëËÄÖR7000', price: 6599, stock: 3, CPU: 'Ryzen 7 5800H', GPU: 'RTX 3050 Ti', type: 'Ê∏∏ÊàèÊú¨', rating: 4.7 },\r\n  { pid: 5, name: 'ËÅîÊÉ≥Yoga Air 14s', price: 7299, stock: 22, CPU: 'i5-1340P', GPU: 'Ê†∏Êòæ', type: 'ËΩªËñÑÊú¨', rating: 4.5 },\r\n  { pid: 6, name: 'ËÅîÊÉ≥GeekPro 2023', price: 5299, stock: 0, CPU: 'i5-13400F', GPU: 'RTX 3060', type: 'Âè∞ÂºèÊú∫', rating: 4.4 },\r\n  { pid: 7, name: 'ËÅîÊÉ≥Â§©ÈÄ∏510S', price: 3999, stock: 12, CPU: 'i3-13100', GPU: 'Ê†∏Êòæ', type: 'Âè∞ÂºèÊú∫', rating: 4.2 },\r\n  { pid: 8, name: 'ËÅîÊÉ≥ThinkBook 14+', price: 5699, stock: 18, CPU: 'i5-13500H', GPU: 'RTX 2050', type: 'ÂÖ®ËÉΩÊú¨', rating: 4.7 }\r\n];\r\n\r\nexport default {\r\n  name: 'AdminProducts',\r\n  data() {\r\n    return {\r\n      productSearch: '',\r\n      showLowStock: false,\r\n      currentPage: 1,\r\n      pageSize: 20,\r\n      selectedProduct: null,\r\n      loading: false,\r\n      error: null,\r\n      isMockData: false,\r\n\r\n      // ÂïÜÂìÅÊï∞ÊçÆ - ÂàùÂßã‰∏∫Á©∫Ôºå‰ªéAPIÂä†ËΩΩ\r\n      products: []\r\n    }\r\n  },\r\n  computed: {\r\n    filteredProducts() {\r\n      let products = this.products;\r\n\r\n      // 1. ÊêúÁ¥¢Á≠õÈÄâ\r\n      if (this.productSearch) {\r\n        const search = this.productSearch.toLowerCase();\r\n        products = products.filter(p =>\r\n            p.name.toLowerCase().includes(search) ||\r\n            (p.type && p.type.toLowerCase().includes(search))\r\n        );\r\n      }\r\n\r\n      // 2. ‰ΩéÂ∫ìÂ≠òÁ≠õÈÄâ\r\n      if (this.showLowStock) {\r\n        products = products.filter(p => p.stock <= 10 && p.stock > 0);\r\n      }\r\n\r\n      // ID ÈôçÂ∫è\r\n      return products.sort((a, b) => b.pid - a.pid);\r\n    },\r\n\r\n    totalPages() {\r\n      return Math.ceil(this.filteredProducts.length / this.pageSize);\r\n    },\r\n    \r\n    paginatedProducts() {\r\n      const start = (this.currentPage - 1) * this.pageSize;\r\n      return this.filteredProducts.slice(start, start + this.pageSize);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadProducts();\r\n  },\r\n  methods: {\r\n    nextPage() {\r\n      if (this.currentPage < this.totalPages) this.currentPage++;\r\n    },\r\n    prevPage() {\r\n      if (this.currentPage > 1) this.currentPage--;\r\n    },\r\n    formatRating(val) {\r\n      if (!val) return 0;\r\n      const num = Number(val);\r\n      // Â¶ÇÊûúËØÑÂàÜÂ§ß‰∫é5ÔºåÂÅáËÆæÊòØÁôæÂàÜÂà∂ÔºàÂ¶Ç95, 100ÔºâÔºåËΩ¨Êç¢‰∏∫5ÂàÜÂà∂\r\n      if (num > 5) {\r\n        const scaled = num / 20;\r\n        // ÊúÄÂ§ö5.0ÂàÜ\r\n        return scaled > 5 ? 5.0 : Number(scaled.toFixed(1));\r\n      }\r\n      return num;\r\n    },\r\n\r\n    getStockLevel(stock) {\r\n      if (stock === 0) return 'out';\r\n      if (stock < 5) return 'low';\r\n      if (stock < 20) return 'medium';\r\n      return 'high';\r\n    },\r\n\r\n    // Âä†ËΩΩÂïÜÂìÅÊï∞ÊçÆ\r\n    async loadProducts() {\r\n      this.loading = true;\r\n      this.error = null;\r\n\r\n      try {\r\n        // Â∞ùËØï‰ªéAPIËé∑ÂèñÊï∞ÊçÆ\r\n        const response = await axios.get(apiConfig.product.getAllProducts);\r\n\r\n        if (response.data.code === 1) {\r\n          // APIËØ∑Ê±ÇÊàêÂäüÔºå‰ΩøÁî®ÁúüÂÆûÊï∞ÊçÆ\r\n          this.products = response.data.data.map(product => ({\r\n            pid: product.pid || product.id,\r\n            name: product.name || product.productName,\r\n            price: product.price || 0,\r\n            stock: product.stock || product.quantity || 0,\r\n            CPU: product.CPU || product.cpu || '-',\r\n            GPU: product.GPU || product.gpu || '-',\r\n            type: product.type || product.category || 'Êú™ÂàÜÁ±ª',\r\n            rating: this.formatRating(product.rating)\r\n          }));\r\n        } else {\r\n          // APIËøîÂõûÈîôËØØ\r\n          throw new Error(response.data.msg || `APIËøîÂõûÈîôËØØ (code=${response.data.code})`);\r\n        }\r\n      } catch (error) {\r\n        console.error('Âä†ËΩΩÂïÜÂìÅÊï∞ÊçÆÂ§±Ë¥•ÔºåÂàáÊç¢Ëá≥Ê®°ÊãüÊï∞ÊçÆ:', error);\r\n        // APIË∞ÉÁî®Â§±Ë¥•Ôºå‰ΩøÁî®Ê®°ÊãüÊï∞ÊçÆ\r\n        this.products = [...MOCK_PRODUCTS];\r\n        this.isMockData = true;\r\n        this.error = 'APIËøûÊé•Â§±Ë¥•ÔºåÂ∑≤ÊòæÁ§∫Ê®°ÊãüÊï∞ÊçÆ';\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    // ÁºñËæëÂïÜÂìÅ\r\n    editProduct(product) {\r\n      // Áî±‰∫éÂêéÁ´ØÊ≤°ÊúâÊèê‰æõÂïÜÂìÅÁºñËæëAPIÔºåÊàë‰ª¨‰ΩøÁî®Ê®°ÊãüÊìç‰Ωú\r\n      this.selectedProduct = { ...product };\r\n      alert(`ÁºñËæëÂïÜÂìÅÔºö${product.name}\\nÂØπÂ∫îAPIÔºöÂïÜÂìÅÁºñËæëÊé•Âè£ÊöÇÊú™Êèê‰æõ`);\r\n    },\r\n\r\n    // Êü•ÁúãÂïÜÂìÅËØ¶ÊÉÖ\r\n    viewProduct(product) {\r\n      this.selectedProduct = { ...product };\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* ‰ΩøÁî®ÂíåÁî®Êà∑ÁÆ°ÁêÜÁõ∏ÂêåÁöÑÊ†∑Âºè‰ΩìÁ≥ªÔºå‰øùÊåÅÁªü‰∏Ä */\r\n\r\n.management-section {\r\n  background: white;\r\n  border: 1px solid #e0e0e0;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Âä†ËΩΩÁä∂ÊÄÅ */\r\n.loading-state {\r\n  padding: 40px 20px;\r\n  text-align: center;\r\n  color: #666;\r\n  background: #fafafa;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  gap: 12px;\r\n}\r\n\r\n.spinner {\r\n  width: 40px;\r\n  height: 40px;\r\n  border: 3px solid #f3f3f3;\r\n  border-top: 3