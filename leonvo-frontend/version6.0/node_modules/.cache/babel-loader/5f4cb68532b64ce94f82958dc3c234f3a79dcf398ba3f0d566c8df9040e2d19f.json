{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.every.js\";\nimport axios from 'axios';\nimport apiConfig from '@/config/api.config.js';\nexport default {\n  name: 'SmsRegister',\n  data() {\n    return {\n      form: {\n        username: '',\n        tel: '',\n        smsCode: '',\n        password: '',\n        confirmPassword: '',\n        agree: false\n      },\n      errors: {\n        username: '',\n        tel: '',\n        smsCode: '',\n        password: '',\n        confirmPassword: '',\n        agree: ''\n      },\n      smsStatus: null,\n      countdown: 0,\n      canSend: true,\n      showSuccess: false,\n      countdownTimer: null\n    };\n  },\n  computed: {\n    isFormValid() {\n      return Object.values(this.errors).every(error => !error) && this.form.username && this.form.tel && this.form.smsCode && this.form.password && this.form.confirmPassword && this.form.agree;\n    }\n  },\n  methods: {\n    async validateUsername() {\n      if (!this.form.username) {\n        this.errors.username = 'Áî®Êà∑Âêç‰∏çËÉΩ‰∏∫Á©∫';\n        return;\n      } else if (this.form.username.length < 2 || this.form.username.length > 10) {\n        this.errors.username = 'Áî®Êà∑ÂêçÈïøÂ∫¶‰∏∫2-10‰Ωç';\n        return;\n      }\n\n      // Ê£ÄÊü•Áî®Êà∑ÂêçÊòØÂê¶Â∑≤Â≠òÂú®ÔºàË∞ÉÁî®ÂêéÁ´ØAPIÔºâ\n      try {\n        const response = await axios.get(apiConfig.user.checkUsername, {\n          params: {\n            username: this.form.username\n          }\n        });\n\n        // code === 0 Ë°®Á§∫Â∑≤Â≠òÂú®\n        if (response.data && response.data.code === 0) {\n          this.errors.username = 'Áî®Êà∑ÂêçÂ∑≤Â≠òÂú®';\n        } else {\n          this.errors.username = '';\n        }\n      } catch (error) {\n        console.error('Ê£ÄÊü•Áî®Êà∑ÂêçÂ§±Ë¥•:', error);\n        this.errors.username = 'Ê£ÄÊü•Áî®Êà∑ÂêçÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï';\n      }\n    },\n    validateTel() {\n      const phoneRegex = /^1[3-9]\\d{9}$/;\n      if (!this.form.tel) {\n        this.errors.tel = 'ÊâãÊú∫Âè∑‰∏çËÉΩ‰∏∫Á©∫';\n      } else if (!phoneRegex.test(this.form.tel)) {\n        this.errors.tel = 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÊâãÊú∫Âè∑';\n      } else {\n        this.errors.tel = '';\n      }\n    },\n    async checkTelExists() {\n      if (!this.form.tel || this.errors.tel) {\n        return false;\n      }\n      try {\n        const response = await axios.get(apiConfig.user.checkPhone, {\n          params: {\n            phone: this.form.tel\n          }\n        });\n\n        // code === 0 Ë°®Á§∫Â∑≤Â≠òÂú®\n        if (response.data && response.data.code === 0) {\n          this.errors.tel = 'ÊâãÊú∫Âè∑Â∑≤Ê≥®ÂÜå';\n          return true;\n        }\n        return false;\n      } catch (error) {\n        console.error('Ê£ÄÊü•ÊâãÊú∫Âè∑Â§±Ë¥•:', error);\n        return false;\n      }\n    },\n    validatePassword() {\n      if (!this.form.password) {\n        this.errors.password = 'ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫';\n      } else if (this.form.password.length < 6 || this.form.password.length > 16) {\n        this.errors.password = 'ÂØÜÁ†ÅÈïøÂ∫¶‰∏∫6-16‰Ωç';\n      } else {\n        this.errors.password = '';\n      }\n    },\n    validateConfirmPassword() {\n      if (!this.form.confirmPassword) {\n        this.errors.confirmPassword = 'ËØ∑Á°ÆËÆ§ÂØÜÁ†Å';\n      } else if (this.form.password !== this.form.confirmPassword) {\n        this.errors.confirmPassword = '‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥';\n      } else {\n        this.errors.confirmPassword = '';\n      }\n    },\n    async sendSmsCode() {\n      // È™åËØÅÊâãÊú∫Âè∑\n      this.validateTel();\n      if (this.errors.tel) {\n        return;\n      }\n\n      // Ê£ÄÊü•ÊâãÊú∫Âè∑ÊòØÂê¶Â∑≤Ê≥®ÂÜå\n      const exists = await this.checkTelExists();\n      if (exists) {\n        return;\n      }\n      this.canSend = false;\n      this.smsStatus = null;\n      try {\n        const response = await axios.post(apiConfig.sms.sendCode, {\n          phone: this.form.tel,\n          purpose: 'register'\n        });\n        const data = response.data || {};\n        if (data.code === 1) {\n          this.smsStatus = {\n            success: true,\n            message: 'È™åËØÅÁ†ÅÂèëÈÄÅÊàêÂäü'\n          };\n          // ÂºÄÂßãÂÄíËÆ°Êó∂60Áßí\n          this.startCountdown(60);\n        } else {\n          this.smsStatus = {\n            success: false,\n            message: data.msg || 'È™åËØÅÁ†ÅÂèëÈÄÅÂ§±Ë¥•'\n          };\n          this.canSend = true;\n\n          // Â¶ÇÊûúÊúâÂâ©‰ΩôÊó∂Èó¥ÔºåÂºÄÂßãÂÄíËÆ°Êó∂\n          const remaining = data.data?.remaining;\n          if (remaining) {\n            this.startCountdown(remaining);\n          }\n        }\n      } catch (error) {\n        console.error('ÂèëÈÄÅÈ™åËØÅÁ†ÅÂ§±Ë¥•:', error);\n        this.smsStatus = {\n          success: false,\n          message: 'ÂèëÈÄÅÈ™åËØÅÁ†ÅÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï'\n        };\n        this.canSend = true;\n      }\n    },\n    startCountdown(seconds) {\n      this.countdown = seconds;\n      this.countdownTimer = setInterval(() => {\n        this.countdown--;\n        if (this.countdown <= 0) {\n          clearInterval(this.countdownTimer);\n          this.canSend = true;\n        }\n      }, 1000);\n    },\n    async handleRegister() {\n      // È™åËØÅÊâÄÊúâÂ≠óÊÆµ\n      await this.validateUsername();\n      this.validateTel();\n      this.validatePassword();\n      this.validateConfirmPassword();\n      if (!this.form.agree) {\n        this.errors.agree = 'ËØ∑ÂêåÊÑèÁî®Êà∑ÂçèËÆÆÂíåÈöêÁßÅÊîøÁ≠ñ';\n        return;\n      } else {\n        this.errors.agree = '';\n      }\n      if (!this.isFormValid) {\n        return;\n      }\n\n      // È™åËØÅÁü≠‰ø°È™åËØÅÁ†Å\n      if (!this.form.smsCode || this.form.smsCode.length !== 6) {\n        this.errors.smsCode = 'ËØ∑ËæìÂÖ•6‰ΩçÈ™åËØÅÁ†Å';\n        return;\n      }\n      try {\n        // ÂÖàÈ™åËØÅÈ™åËØÅÁ†Å\n        const verifyRes = await axios.post(apiConfig.sms.verifyCode, {\n          phone: this.form.tel,\n          code: this.form.smsCode\n        });\n        if (!verifyRes.data || verifyRes.data.code !== 1) {\n          const msg = verifyRes.data?.msg || 'È™åËØÅÁ†ÅÈîôËØØÊàñÂ∑≤ËøáÊúü';\n          this.errors.smsCode = msg;\n          alert('Ê≥®ÂÜåÂ§±Ë¥•: ' + msg);\n          return;\n        }\n\n        // È™åËØÅÈÄöËøáÂêéÊ≥®ÂÜåÁî®Êà∑\n        const registerRes = await axios.post(apiConfig.user.addUser, {\n          username: this.form.username,\n          password: this.form.password,\n          phone: this.form.tel,\n          status: 1\n        });\n        if (registerRes.data && registerRes.data.code === 1) {\n          // ÊòæÁ§∫ÊàêÂäüÊèêÁ§∫\n          this.showSuccess = true;\n\n          // 3ÁßíÂêéË∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ\n          setTimeout(() => {\n            this.$router.push('/login');\n          }, 3000);\n        } else {\n          const msg = registerRes.data?.msg || 'Ê≥®ÂÜåÂ§±Ë¥•';\n          alert('Ê≥®ÂÜåÂ§±Ë¥•: ' + msg);\n          if (msg.includes && msg.includes('Áî®Êà∑Âêç')) {\n            this.errors.username = msg;\n          } else if (msg.includes && msg.includes('ÊâãÊú∫Âè∑')) {\n            this.errors.tel = msg;\n          }\n        }\n      } catch (error) {\n        console.error('Ê≥®ÂÜåÂ§±Ë¥•:', error);\n        alert('Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï');\n      }\n    }\n  },\n  beforeDestroy() {\n    if (this.countdownTimer) {\n      clearInterval(this.countdownTimer);\n    }\n  }\n};","map":{"version":3,"names":["axios","apiConfig","name","data","form","username","tel","smsCode","password","confirmPassword","agree","errors","smsStatus","countdown","canSend","showSuccess","countdownTimer","computed","isFormValid","Object","values","every","error","methods","validateUsername","length","response","get","user","checkUsername","params","code","console","validateTel","phoneRegex","test","checkTelExists","checkPhone","phone","validatePassword","validateConfirmPassword","sendSmsCode","exists","post","sms","sendCode","purpose","success","message","startCountdown","msg","remaining","seconds","setInterval","clearInterval","handleRegister","verifyRes","verifyCode","alert","registerRes","addUser","status","setTimeout","$router","push","includes","beforeDestroy"],"sources":["D:\\1_homework_file\\Â§ß‰∏â‰∏ä\\ËΩØ‰ª∂Â∑•Á®ã\\È°πÁõÆÁïôÊ°£\\1ÂâçÁ´ØÂºÄÂèë_ÊàëÊâçÊòØÁúüÁöÑ\\version5.0\\src\\views\\user\\TelRegister.vue"],"sourcesContent":["<template>\r\n  <div class=\"register-view\">\r\n    <div class=\"register-container\">\r\n      <div class=\"back-home\">\r\n        <router-link to=\"/login\" class=\"back-link\">\r\n          <span class=\"back-icon\">‚Üê</span>\r\n          ËøîÂõûÁôªÂΩï\r\n        </router-link>\r\n      </div>\r\n\r\n      <div class=\"register-box\">\r\n        <div class=\"register-header\">\r\n          <h1>Áü≠‰ø°È™åËØÅÊ≥®ÂÜå</h1>\r\n          <p class=\"subtitle\">ÂÆâÂÖ®Âø´ÈÄüÂä†ÂÖ•Ê≥°Ê≥°ËÅîÁõüËÅîÊÉ≥ÁîµËÑëÂïÜÂüé</p>\r\n          <div class=\"eco-decoration\">\r\n            <span class=\"leaf-icon\">üì±</span>\r\n            <span class=\"leaf-icon\">üîí</span>\r\n            <span class=\"leaf-icon\">‚úÖ</span>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"register-form\">\r\n          <div class=\"form-group\">\r\n            <label for=\"username\">\r\n              <span class=\"label-icon\">üë§</span>\r\n              Áî®Êà∑ÂêçÔºö\r\n            </label>\r\n            <div class=\"input-wrapper\">\r\n              <input\r\n                  type=\"text\"\r\n                  id=\"username\"\r\n                  v-model=\"form.username\"\r\n                  placeholder=\"ËØ∑ËæìÂÖ•Áî®Êà∑Âêç(2-10‰Ωç)\"\r\n                  class=\"form-input\"\r\n                  @blur=\"validateUsername\"\r\n              />\r\n              <div class=\"input-decoration\">\r\n                <div class=\"input-line\"></div>\r\n              </div>\r\n              <div v-if=\"errors.username\" class=\"error-message\">\r\n                {{ errors.username }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"tel\">\r\n              <span class=\"label-icon\">üì±</span>\r\n              ÊâãÊú∫Âè∑Ôºö\r\n            </label>\r\n            <div class=\"input-wrapper\">\r\n              <input\r\n                  type=\"tel\"\r\n                  id=\"tel\"\r\n                  v-model=\"form.tel\"\r\n                  placeholder=\"ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑\"\r\n                  class=\"form-input\"\r\n                  @blur=\"validateTel\"\r\n              />\r\n              <div class=\"input-decoration\">\r\n                <div class=\"input-line\"></div>\r\n              </div>\r\n              <div v-if=\"errors.tel\" class=\"error-message\">\r\n                {{ errors.tel }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"smsCode\">\r\n              <span class=\"label-icon\">üî¢</span>\r\n              È™åËØÅÁ†ÅÔºö\r\n            </label>\r\n            <div class=\"sms-code-wrapper\">\r\n              <div class=\"sms-code-input\">\r\n                <input\r\n                    type=\"text\"\r\n                    id=\"smsCode\"\r\n                    v-model=\"form.smsCode\"\r\n                    placeholder=\"ËØ∑ËæìÂÖ•6‰ΩçÈ™åËØÅÁ†Å\"\r\n                    class=\"form-input\"\r\n                    maxlength=\"6\"\r\n                />\r\n                <div class=\"input-decoration\">\r\n                  <div class=\"input-line\"></div>\r\n                </div>\r\n              </div>\r\n              <button\r\n                  @click=\"sendSmsCode\"\r\n                  :disabled=\"!canSend || countdown > 0\"\r\n                  class=\"send-code-btn\"\r\n                  :class=\"{ 'disabled-btn': !canSend || countdown > 0 }\"\r\n              >\r\n                <span v-if=\"countdown > 0\">{{ countdown }}sÂêéÈáçÂèë</span>\r\n                <span v-else>ÂèëÈÄÅÈ™åËØÅÁ†Å</span>\r\n              </button>\r\n            </div>\r\n            <div v-if=\"errors.smsCode\" class=\"error-message\">\r\n              {{ errors.smsCode }}\r\n            </div>\r\n            <div v-if=\"smsStatus\" class=\"sms-status\" :class=\"{ 'success': smsStatus.success, 'error': !smsStatus.success }\">\r\n              {{ smsStatus.message }}\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"password\">\r\n              <span class=\"label-icon\">üîí</span>\r\n              ÂØÜÁ†ÅÔºö\r\n            </label>\r\n            <div class=\"input-wrapper\">\r\n              <input\r\n                  type=\"password\"\r\n                  id=\"password\"\r\n                  v-model=\"form.password\"\r\n                  placeholder=\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å(6-16‰Ωç)\"\r\n                  class=\"form-input\"\r\n                  @blur=\"validatePassword\"\r\n              />\r\n              <div class=\"input-decoration\">\r\n                <div class=\"input-line\"></div>\r\n              </div>\r\n              <div v-if=\"errors.password\" class=\"error-message\">\r\n                {{ errors.password }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"confirmPassword\">\r\n              <span class=\"label-icon\">‚úÖ</span>\r\n              Á°ÆËÆ§ÂØÜÁ†ÅÔºö\r\n            </label>\r\n            <div class=\"input-wrapper\">\r\n              <input\r\n                  type=\"password\"\r\n                  id=\"confirmPassword\"\r\n                  v-model=\"form.confirmPassword\"\r\n                  placeholder=\"ËØ∑ÂÜçÊ¨°ËæìÂÖ•ÂØÜÁ†Å\"\r\n                  class=\"form-input\"\r\n                  @blur=\"validateConfirmPassword\"\r\n              />\r\n              <div class=\"input-decoration\">\r\n                <div class=\"input-line\"></div>\r\n              </div>\r\n              <div v-if=\"errors.confirmPassword\" class=\"error-message\">\r\n                {{ errors.confirmPassword }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"form-group remember\">\r\n            <label class=\"checkbox-label\">\r\n              <input\r\n                  type=\"checkbox\"\r\n                  id=\"agree\"\r\n                  v-model=\"form.agree\"\r\n                  class=\"checkbox\"\r\n              />\r\n              <span class=\"checkmark\"></span>\r\n              <span class=\"checkbox-text\">ÊàëÂ∑≤ÈòÖËØªÂπ∂ÂêåÊÑè„ÄäÁî®Êà∑ÂçèËÆÆ„ÄãÂíå„ÄäÈöêÁßÅÊîøÁ≠ñ„Äã</span>\r\n            </label>\r\n            <div v-if=\"errors.agree\" class=\"error-message\">\r\n              {{ errors.agree }}\r\n            </div>\r\n          </div>\r\n\r\n          <button\r\n              @click=\"handleRegister\"\r\n              :disabled=\"!isFormValid\"\r\n              class=\"register-button\"\r\n              :class=\"{ 'button-disabled': !isFormValid }\"\r\n          >\r\n            <span class=\"button-icon\">üì±</span>\r\n            <span class=\"button-text\">Áü≠‰ø°È™åËØÅÊ≥®ÂÜå</span>\r\n          </button>\r\n\r\n          <div class=\"login-tip\">\r\n            <span class=\"tip-icon\">üîê</span>\r\n            Â∑≤ÊúâË¥¶Âè∑Ôºü\r\n            <router-link to=\"/login\" class=\"login-link\">\r\n              Á´ãÂç≥ÁôªÂΩï\r\n            </router-link>\r\n            <span class=\"separator\">|</span>\r\n            <router-link to=\"/register\" class=\"other-register-link\">\r\n              <span class=\"normal-icon\">üë§</span>\r\n              ÈÇÆÁÆ±È™åËØÅÊ≥®ÂÜå\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-if=\"showSuccess\" class=\"success-message\">\r\n        <div class=\"success-content\">\r\n          <div class=\"success-icon\">‚úÖ</div>\r\n          <h3>Ê≥®ÂÜåÊàêÂäüÔºÅ</h3>\r\n          <p>Ê¨¢Ëøé {{ form.username }} üéâ</p>\r\n          <div class=\"welcome-bubble\">\r\n            üì± {{ form.username }} Â∑≤ÈÄöËøáÁü≠‰ø°È™åËØÅÂä†ÂÖ•Ê≥°Ê≥°ËÅîÁõü\r\n          </div>\r\n          <div class=\"success-desc\">\r\n            <span class=\"desc-icon\">‚è≥</span>\r\n            Ê≠£Âú®Ë∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ...\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport apiConfig from '@/config/api.config.js';\r\n\r\nexport default {\r\n  name: 'SmsRegister',\r\n  data() {\r\n    return {\r\n      form: {\r\n        username: '',\r\n        tel: '',\r\n        smsCode: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        agree: false\r\n      },\r\n      errors: {\r\n        username: '',\r\n        tel: '',\r\n        smsCode: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        agree: ''\r\n      },\r\n      smsStatus: null,\r\n      countdown: 0,\r\n      canSend: true,\r\n      showSuccess: false,\r\n      countdownTimer: null\r\n    };\r\n  },\r\n  computed: {\r\n    isFormValid() {\r\n      return Object.values(this.errors).every(error => !error) &&\r\n          this.form.username &&\r\n          this.form.tel &&\r\n          this.form.smsCode &&\r\n          this.form.password &&\r\n          this.form.confirmPassword &&\r\n          this.form.agree;\r\n    }\r\n  },\r\n  methods: {\r\n    async validateUsername() {\r\n      if (!this.form.username) {\r\n        this.errors.username = 'Áî®Êà∑Âêç‰∏çËÉΩ‰∏∫Á©∫';\r\n        return;\r\n      } else if (this.form.username.length < 2 || this.form.username.length > 10) {\r\n        this.errors.username = 'Áî®Êà∑ÂêçÈïøÂ∫¶‰∏∫2-10‰Ωç';\r\n        return;\r\n      }\r\n\r\n      // Ê£ÄÊü•Áî®Êà∑ÂêçÊòØÂê¶Â∑≤Â≠òÂú®ÔºàË∞ÉÁî®ÂêéÁ´ØAPIÔºâ\r\n      try {\r\n        const response = await axios.get(apiConfig.user.checkUsername, {\r\n          params: { username: this.form.username }\r\n        });\r\n\r\n        // code === 0 Ë°®Á§∫Â∑≤Â≠òÂú®\r\n        if (response.data && response.data.code === 0) {\r\n          this.errors.username = 'Áî®Êà∑ÂêçÂ∑≤Â≠òÂú®';\r\n        } else {\r\n          this.errors.username = '';\r\n        }\r\n      } catch (error) {\r\n        console.error('Ê£ÄÊü•Áî®Êà∑ÂêçÂ§±Ë¥•:', error);\r\n        this.errors.username = 'Ê£ÄÊü•Áî®Êà∑ÂêçÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï';\r\n      }\r\n    },\r\n\r\n    validateTel() {\r\n      const phoneRegex = /^1[3-9]\\d{9}$/;\r\n      if (!this.form.tel) {\r\n        this.errors.tel = 'ÊâãÊú∫Âè∑‰∏çËÉΩ‰∏∫Á©∫';\r\n      } else if (!phoneRegex.test(this.form.tel)) {\r\n        this.errors.tel = 'ËØ∑ËæìÂÖ•ÊúâÊïàÁöÑÊâãÊú∫Âè∑';\r\n      } else {\r\n        this.errors.tel = '';\r\n      }\r\n    },\r\n\r\n    async checkTelExists() {\r\n      if (!this.form.tel || this.errors.tel) {\r\n        return false;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.get(apiConfig.user.checkPhone, {\r\n          params: { phone: this.form.tel }\r\n        });\r\n\r\n        // code === 0 Ë°®Á§∫Â∑≤Â≠òÂú®\r\n        if (response.data && response.data.code === 0) {\r\n          this.errors.tel = 'ÊâãÊú∫Âè∑Â∑≤Ê≥®ÂÜå';\r\n          return true;\r\n        }\r\n        return false;\r\n      } catch (error) {\r\n        console.error('Ê£ÄÊü•ÊâãÊú∫Âè∑Â§±Ë¥•:', error);\r\n        return false;\r\n      }\r\n    },\r\n\r\n    validatePassword() {\r\n      if (!this.form.password) {\r\n        this.errors.password = 'ÂØÜÁ†Å‰∏çËÉΩ‰∏∫Á©∫';\r\n      } else if (this.form.password.length < 6 || this.form.password.length > 16) {\r\n        this.errors.password = 'ÂØÜÁ†ÅÈïøÂ∫¶‰∏∫6-16‰Ωç';\r\n      } else {\r\n        this.errors.password = '';\r\n      }\r\n    },\r\n\r\n    validateConfirmPassword() {\r\n      if (!this.form.confirmPassword) {\r\n        this.errors.confirmPassword = 'ËØ∑Á°ÆËÆ§ÂØÜÁ†Å';\r\n      } else if (this.form.password !== this.form.confirmPassword) {\r\n        this.errors.confirmPassword = '‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥';\r\n      } else {\r\n        this.errors.confirmPassword = '';\r\n      }\r\n    },\r\n\r\n    async sendSmsCode() {\r\n      // È™åËØÅÊâãÊú∫Âè∑\r\n      this.validateTel();\r\n      if (this.errors.tel) {\r\n        return;\r\n      }\r\n\r\n      // Ê£ÄÊü•ÊâãÊú∫Âè∑ÊòØÂê¶Â∑≤Ê≥®ÂÜå\r\n      const exists = await this.checkTelExists();\r\n      if (exists) {\r\n        return;\r\n      }\r\n\r\n      this.canSend = false;\r\n      this.smsStatus = null;\r\n\r\n      try {\r\n        const response = await axios.post(apiConfig.sms.sendCode, {\r\n          phone: this.form.tel,\r\n          purpose: 'register'\r\n        });\r\n\r\n        const data = response.data || {};\r\n\r\n        if (data.code === 1) {\r\n          this.smsStatus = {\r\n            success: true,\r\n            message: 'È™åËØÅÁ†ÅÂèëÈÄÅÊàêÂäü'\r\n          };\r\n          // ÂºÄÂßãÂÄíËÆ°Êó∂60Áßí\r\n          this.startCountdown(60);\r\n        } else {\r\n          this.smsStatus = {\r\n            success: false,\r\n            message: data.msg || 'È™åËØÅÁ†ÅÂèëÈÄÅÂ§±Ë¥•'\r\n          };\r\n          this.canSend = true;\r\n\r\n          // Â¶ÇÊûúÊúâÂâ©‰ΩôÊó∂Èó¥ÔºåÂºÄÂßãÂÄíËÆ°Êó∂\r\n          const remaining = data.data?.remaining;\r\n          if (remaining) {\r\n            this.startCountdown(remaining);\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error('ÂèëÈÄÅÈ™åËØÅÁ†ÅÂ§±Ë¥•:', error);\r\n        this.smsStatus = {\r\n          success: false,\r\n          message: 'ÂèëÈÄÅÈ™åËØÅÁ†ÅÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï'\r\n        };\r\n        this.canSend = true;\r\n      }\r\n    },\r\n\r\n    startCountdown(seconds) {\r\n      this.countdown = seconds;\r\n\r\n      this.countdownTimer = setInterval(() => {\r\n        this.countdown--;\r\n\r\n        if (this.countdown <= 0) {\r\n          clearInterval(this.countdownTimer);\r\n          this.canSend = true;\r\n        }\r\n      }, 1000);\r\n    },\r\n\r\n    async handleRegister() {\r\n      // È™åËØÅÊâÄÊúâÂ≠óÊÆµ\r\n      await this.validateUsername();\r\n      this.validateTel();\r\n      this.validatePassword();\r\n      this.validateConfirmPassword();\r\n\r\n      if (!this.form.agree) {\r\n        this.errors.agree = 'ËØ∑ÂêåÊÑèÁî®Êà∑ÂçèËÆÆÂíåÈöêÁßÅÊîøÁ≠ñ';\r\n        return;\r\n      } else {\r\n        this.errors.agree = '';\r\n      }\r\n\r\n      if (!this.isFormValid) {\r\n        return;\r\n      }\r\n\r\n      // È™åËØÅÁü≠‰ø°È™åËØÅÁ†Å\r\n      if (!this.form.smsCode || this.form.smsCode.length !== 6) {\r\n        this.errors.smsCode = 'ËØ∑ËæìÂÖ•6‰ΩçÈ™åËØÅÁ†Å';\r\n        return;\r\n      }\r\n\r\n      try {\r\n        // ÂÖàÈ™åËØÅÈ™åËØÅÁ†Å\r\n        const verifyRes = await axios.post(apiConfig.sms.verifyCode, {\r\n          phone: this.form.tel,\r\n          code: this.form.smsCode\r\n        });\r\n\r\n        if (!verifyRes.data || verifyRes.data.code !== 1) {\r\n          const msg = verifyRes.data?.msg || 'È™åËØÅÁ†ÅÈîôËØØÊàñÂ∑≤ËøáÊúü';\r\n          this.errors.smsCode = msg;\r\n          alert('Ê≥®ÂÜåÂ§±Ë¥•: ' + msg);\r\n          return;\r\n        }\r\n\r\n        // È™åËØÅÈÄöËøáÂêéÊ≥®ÂÜåÁî®Êà∑\r\n        const registerRes = await axios.post(apiConfig.user.addUser, {\r\n          username: this.form.username,\r\n          password: this.form.password,\r\n          phone: this.form.tel,\r\n          status: 1\r\n        });\r\n\r\n        if (registerRes.data && registerRes.data.code === 1) {\r\n          // ÊòæÁ§∫ÊàêÂäüÊèêÁ§∫\r\n          this.showSuccess = true;\r\n\r\n          // 3ÁßíÂêéË∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ\r\n          setTimeout(() => {\r\n            this.$router.push('/login');\r\n          }, 3000);\r\n        } else {\r\n          const msg = registerRes.data?.msg || 'Ê≥®ÂÜåÂ§±Ë¥•';\r\n          alert('Ê≥®ÂÜåÂ§±Ë¥•: ' + msg);\r\n          if (msg.includes && msg.includes('Áî®Êà∑Âêç')) {\r\n            this.errors.username = msg;\r\n          } else if (msg.includes && msg.includes('ÊâãÊú∫Âè∑')) {\r\n            this.errors.tel = msg;\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error('Ê≥®ÂÜåÂ§±Ë¥•:', error);\r\n        alert('Ê≥®ÂÜåÂ§±Ë¥•ÔºåËØ∑Á®çÂêéÈáçËØï');\r\n      }\r\n    }\r\n  },\r\n  beforeDestroy() {\r\n    if (this.countdownTimer) {\r\n      clearInterval(this.countdownTimer);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.register-view {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: linear-gradient(135deg, #e3f2fd 0%, #bbdefb 30%, #90caf9 70%, #64b5f6 100%);\r\n  padding: 20px;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.register-container {\r\n  width: 100%;\r\n  max-width: 500px;\r\n  position: relative;\r\n  z-index: 2;\r\n}\r\n\r\n.register-box {\r\n  background: white;\r\n  border-radius: 0;\r\n  padding: 50px;\r\n  box-shadow: 0 10px 40px rgba(75, 108, 183, 0.2);\r\n  border: 2px solid #4b6cb7;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.register-header {\r\n  text-align: center;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.register-header h1 {\r\n  color: #182848;\r\n  margin: 0 0 15px 0;\r\n  font-size: 36px;\r\n  font-weight: 900;\r\n  text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.1);\r\n  letter-spacing: 0.5px;\r\n  position: relative;\r\n}\r\n\r\n.register-header h1::after {\r\n  content: '';\r\n  position: absolute;\r\n  bottom: -8px;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  width: 60px;\r\n  height: 3px;\r\n  background: linear-gradient(90deg, #4b6cb7, #182848);\r\n  border-radius: 0;\r\n}\r\n\r\n.subtitle {\r\n  color: #4b6cb7;\r\n  font-size: 16px;\r\n  margin: 0;\r\n}\r\n\r\n.eco-decoration {\r\n  margin-top: 10px;\r\n}\r\n\r\n.leaf-icon {\r\n  font-size: 20px;\r\n  margin: 0 5px;\r\n}\r\n\r\n.register-form {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n}\r\n\r\n.form-group {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.form-group label {\r\n  display: 